      "text/plain": [
       "swaptruck1 (generic function with 1 method)"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "using Random\n",
    "using StatsBase  # Needed for sample()\n",
    "\n",
    "function swaptruck1(model, Sol, flag)\n",
    "    nNodes = model[\"nNodes\"]\n",
    "    ua = model[\"uav\"]\n",
    "    tru = model[\"tru\"]\n",
    "    L = model[\"L\"]\n",
    "    Du = model[\"Du\"]\n",
    "    Dt = model[\"Dt\"]\n",
    "\n",
    "    TruckRoute = Sol[\"TruckRoute\"]\n",
    "    DroneRoute = Sol[\"DroneRoute\"]\n",
    "    flag = 0\n",
    "\n",
    "    truckroute = copy(TruckRoute)\n",
    "    for i in 1:tru\n",
    "        last_nonzero = findlast(!iszero, truckroute[i, :])\n",
    "        if last_nonzero !== nothing\n",
    "            truckroute[i, last_nonzero] = 0\n",
    "        end\n",
    "    end\n",
    "    truckroute[:, 1] .= 0  # Remove the initial depot\n",
    "\n",
    "    uav = Int[]\n",
    "    \n",
    "    #added:\n",
    "    for u in 1:ua\n",
    "        satr = findall(x -> x != 0, DroneRoute[:, 1, u])  # Find the launch node\n",
    "        if !isempty(satr)  # Check if UAV has a flight\n",
    "            push!(uav, u)            \n",
    "            for in in 1:length(satr)                \n",
    "                indices = findall(x -> x == DroneRoute[satr[in], 1, u], truckroute)\n",
    "                if !isempty(indices)\n",
    "                    (aa, bb) = Tuple(indices[1])\n",
    "                    truckroute[aa, bb] = 0\n",
    "                else\n",
    "                    aa, bb = 0, 0  # Assign safe defaults\n",
    "                    \n",
    "                end\n",
    "\n",
    "                last_index = findlast(!iszero, DroneRoute[satr[in], :, u])\n",
    "                indices = findall(x -> x == DroneRoute[satr[in], last_index, u], truckroute)\n",
    "                if !isempty(indices)\n",
    "                    (cc, dd) = Tuple(indices[1])\n",
    "                    truckroute[cc, dd] = 0\n",
    "                else\n",
    "                    cc, dd = 0, 0  # Assign safe defaults\n",
    "                end                \n",
    "                \n",
    "            end\n",
    "        end\n",
    "    end\n",
    "    nonzero = findall(!iszero, truckroute)\n",
    "    if length(nonzero) < 2\n",
    "        flag = 1\n",
    "        return Sol, flag\n",
    "    end\n",
    "    \n",
    "    t = sample(1:length(nonzero), 2, replace=false)\n",
    "    a, b = truckroute[nonzero[t[1]]], truckroute[nonzero[t[2]]]\n",
    "    \n",
    "    TruckRoute[nonzero[t[1]]] = b\n",
    "    TruckRoute[nonzero[t[2]]] = a    \n",
    "\n",
    "    Sol[\"TruckRoute\"] = TruckRoute\n",
    "    Sol[\"DroneRoute\"] = DroneRoute\n",
    "    return Sol, flag\n",
    "end\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "swaptrone1 (generic function with 1 method)"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "function swaptrone1(model, Sol, flag)\n",
    "\n",
    "    nNodes = model[\"nNodes\"]\n",
    "    ua = model[\"uav\"]\n",
    "    tru = model[\"tru\"]\n",
    "    L = model[\"L\"]\n",
    "    Du = model[\"Du\"]\n",
    "    Dt = model[\"Dt\"]\n",
    "    \n",
    "    TruckRoute = Sol[\"TruckRoute\"]\n",
    "    DroneRoute = Sol[\"DroneRoute\"]\n",
    "    \n",
    "    flag = 0\n",
    "\n",
    "    truckroute = copy(TruckRoute)\n",
    "    for i in 1:tru\n",
    "        truckroute[i, count(!iszero, truckroute[i, :])] = 0\n",
    "    end\n",
    "    truckroute[:, 1] .= 0\n",
    "\n",
    "    uav = []\n",
    "    \n",
    "    #added:\n",
    "    for u in 1:ua                \n",
    "        satr = findall(!iszero, DroneRoute[:, 1, u])\n",
    "        if !isempty(satr)\n",
    "            push!(uav, u)            \n",
    "\n",
    "            for in in 1:length(satr)                \n",
    "                indices = findfirst(x -> x == DroneRoute[satr[in], 1, u], truckroute)                  \n",
    "                if indices !== nothing  \n",
    "                    aa, bb = Tuple(indices)  \n",
    "                    truckroute[aa, bb] = 0\n",
    "                end            \n",
    "                indices = findfirst(x -> x == DroneRoute[satr[in], count(!iszero, DroneRoute[satr[in], :, u]), u], truckroute)  \n",
    "                if indices !== nothing  \n",
    "                    cc, dd = Tuple(indices)  \n",
    "                    truckroute[cc, dd] = 0\n",
    "                end                       \n",
    "            end\n",
    "        end\n",
    "    end\n",
    "    indices = findall(!iszero, truckroute)  # Finds all (row, col) positions\n",
    "\n",
    "    if isempty(indices)\n",
    "        flag = 1\n",
    "        return Sol, flag\n",
    "    else\n",
    "        roww = [idx[1] for idx in indices]  # Extract row indices\n",
    "        coll = [idx[2] for idx in indices]  # Extract column indices\n",
    "    \n",
    "        rnd = rand(1:length(roww))\n",
    "        trex = TruckRoute[roww[rnd], coll[rnd]]\n",
    "    end\n",
    "    \n",
    "\n",
    "    if isempty(uav)\n",
    "        flag = 1\n",
    "        return Sol, flag\n",
    "    end\n",
    "\n",
    "    ii = rand(1:length(uav))\n",
    "    row = findall(!iszero, DroneRoute[:, 1, uav[ii]])\n",
    "    ro = rand(1:length(row))\n",
    "    launch = DroneRoute[row[ro], 1:count(!iszero, DroneRoute[row[ro], :, uav[ii]]), uav[ii]]\n",
    "    alternative = copy(launch)\n",
    "    deleteat!(alternative, 1)\n",
    "    deleteat!(alternative, length(alternative))\n",
    "\n",
    "    for i in 1:length(alternative)\n",
    "        sum = 0        \n",
    "        sotoon = findfirst(x -> x == alternative[i], launch)         \n",
    "        sum += Du[DroneRoute[row[ro], sotoon-1, uav[ii]], trex] + Du[trex, DroneRoute[row[ro], sotoon+1, uav[ii]]]\n",
    "        for j in 1:length(launch)-1\n",
    "            if j == sotoon || j+1 == sotoon\n",
    "                continue\n",
    "            end\n",
    "            sum += Du[DroneRoute[row[ro], j, uav[ii]], DroneRoute[row[ro], j+1, uav[ii]]]\n",
    "        end\n",
    "\n",
    "        if sum <= L\n",
    "            TruckRoute[roww[rnd], coll[rnd]] = DroneRoute[row[ro], sotoon, uav[ii]]\n",
    "            DroneRoute[row[ro], sotoon, uav[ii]] = trex\n",
    "            break\n",
    "        end\n",
    "    end\n",
    "    \n",
    "    Sol[\"TruckRoute\"] = TruckRoute\n",
    "    Sol[\"DroneRoute\"] = DroneRoute\n",
    "    return Sol, flag\n",
    "end\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 26,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "InsertionDt1 (generic function with 1 method)"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "function InsertionDt1(model, Sol, flag)\n",
    "    nNodes = model[\"nNodes\"]\n",
    "    ua = model[\"uav\"]\n",
    "    tru = model[\"tru\"]\n",
    "    L = model[\"L\"]\n",
    "    Du = model[\"Du\"]\n",
    "    Du = model[\"Dt\"]\n",
    "\n",
    "    # Extract solution parameters\n",
    "    TruckRoute = Sol[\"TruckRoute\"]\n",
    "    DroneRoute = Sol[\"DroneRoute\"]\n",
    "    \n",
    "    flag = 0\n",
    "\n",
    "    truckroute2=copy(TruckRoute)\n",
    "\n",
    "    uav = []\n",
    "    for u in 1:ua\n",
    "        if !isempty(findall(!iszero, DroneRoute[:, :, u]))\n",
    "            push!(uav, u)\n",
    "        end\n",
    "    end\n",
    "\n",
    "    if isempty(uav)\n",
    "        flag = 1\n",
    "        return Sol, flag\n",
    "    end\n",
    "\n",
    "    uu = rand(1:length(uav)) \n",
    "    Drone = []\n",
    "\n",
    "    satr = findall(!iszero, DroneRoute[:, 1, uav[uu]])\n",
    "    for i in 1:length(satr)\n",
    "        Drone = vcat(Drone, DroneRoute[satr[i], 2:count(!iszero, DroneRoute[satr[i], :, uav[uu]])-1, uav[uu]])\n",
    "    end\n",
    "\n",
    "    rnd = rand(1:length(Drone))\n",
    "    Dronenode = Drone[rnd]\n",
    "\n",
    "    if length(satr) == 1\n",
    "        indices = findfirst(DroneRoute[:, :, uav[uu]] .== Dronenode)\n",
    "        h, g = Tuple(indices)\n",
    "        if count(!iszero, DroneRoute[h, :, uav[uu]]) == 3\n",
    "            DroneRoute[h, :, uav[uu]] .= 0\n",
    "        else\n",
    "            DroneRoute[h, g:end-1, uav[uu]] = DroneRoute[h, g+1:end, uav[uu]]\n",
    "            DroneRoute[h, end, uav[uu]] = 0\n",
    "        end\n",
    "    else\n",
    "        indices = findfirst(DroneRoute[:, :, uav[uu]] .== Dronenode)\n",
    "        h, g = Tuple(indices)        \n",
    "        if count(!iszero, DroneRoute[h, :, uav[uu]]) == 3\n",
    "            DroneRoute[h, :, uav[uu]] .= 0\n",
    "            if h != length(satr)\n",
    "                cc = 0\n",
    "                if h != 1\n",
    "                    if count(!iszero, DroneRoute[h-1, :, uav[uu]])==0\n",
    "                        flag = 1\n",
    "                        return Sol, flag                        \n",
    "                    end                    \n",
    "                    ghabli_indices = findfirst(TruckRoute .== DroneRoute[h-1, count(!iszero, DroneRoute[h-1, :, uav[uu]]), uav[uu]])\n",
    "                    ghabli = Tuple(ghabli_indices)\n",
    "                    badi_indices = findfirst(TruckRoute .== DroneRoute[h+1, 1, uav[uu]])\n",
    "                    badi = Tuple(badi_indices)\n",
    "                    if ghabli != badi\n",
    "                        sod_sot_indices = findfirst(TruckRoute .== DroneRoute[h+1, 1, uav[uu]])\n",
    "                        sod, sot = Tuple(sod_sot_indices)\n",
    "                        land = TruckRoute[sod, 1:sot]\n",
    "                        land = land[land .!= 0]\n",
    "                        land = land[land .!= 1]\n",
    "                        cost = DroneRoute[h-1, 1:count(!iszero, DroneRoute[h-1, :, uav[uu]]), uav[uu]]\n",
    "                        cost = cost[1:end-1]\n",
    "                        hazine = 0\n",
    "                        for jz in 1:length(cost)-1\n",
    "                            hazine += Du[cost[jz], cost[jz+1]]\n",
    "                        end\n",
    "                        include = []\n",
    "                        for iz in land\n",
    "                            flightrange = hazine + Du[cost[end], iz]\n",
    "                            if flightrange <= L\n",
    "                                push!(include, iz)\n",
    "                            end\n",
    "                        end\n",
    "                        if isempty(include)\n",
    "                            flag = 1\n",
    "                            return Sol, flag\n",
    "                        end\n",
    "                        distance = zeros(nNodes, nNodes)\n",
    "                        for iv in 1:nNodes                            \n",
    "\n",
    "                            if !isempty(findall(x -> x == iv, TruckRoute)) # node in truck route\n",
    "                                if iv == 1\n",
    "                                    for t in 1:tru\n",
    "                                        distance[iv, TruckRoute[t, 1+1]] = 1\n",
    "                                    end\n",
    "                                else\n",
    "                                    fa_ja = findfirst(x -> x == iv, TruckRoute)\n",
    "                                    fa,ja=Tuple(fa_ja)\n",
    "                                    if fa !== nothing && ja !== nothing\n",
    "                                        distance[iv, TruckRoute[fa, ja+1]] = 1\n",
    "                                    end\n",
    "                                end\n",
    "                            end\n",
    "                            \n",
    "                            #added:\n",
    "                            ind = findall(x -> x == iv, DroneRoute)\n",
    "                            if !isempty(ind)                                \n",
    "                                tuples = [Tuple(CartesianIndices(DroneRoute)[ik]) for ik in ind]\n",
    "                                q1 = getindex.(tuples, 1)\n",
    "                                q2 = getindex.(tuples, 2)\n",
    "                                q3 = getindex.(tuples, 3)\n",
    "                                for u in 1:length(q3)\n",
    "                                    if q2[u] != count(!iszero, DroneRoute[q1[u], :, q3[u]])\n",
    "                                        distance[iv, DroneRoute[q1[u], q2[u]+1, q3[u]]] = 2\n",
    "                                    end\n",
    "                                end\n",
    "                            end                        \n",
    "                        end\n",
    "                        graphic = Graphs.DiGraph(distance)\n",
    "                        s = collect(all_simple_paths(graphic, 1, DroneRoute[h-1, 1, uav[uu]]))\n",
    "                        aa = []\n",
    "                        for ii in 1:length(s)\n",
    "                            aa = vcat(aa, s[ii])\n",
    "                        end\n",
    "                        for jj in 1:length(aa)\n",
    "                            include = include[include .!= aa[jj]]\n",
    "                        end\n",
    "                        if isempty(include)\n",
    "                            flag = 1\n",
    "                            return Sol, flag\n",
    "                        end\n",
    "                        if !isempty(include)\n",
    "                            no = rand(1:length(include))\n",
    "                            DroneRoute[h-1, count(!iszero, DroneRoute[h-1, :, uav[uu]]), uav[uu]] = include[no]\n",
    "                            cc = 1\n",
    "                        end\n",
    "                        if cc == 1\n",
    "                            baze = h:satr[end]-1\n",
    "                            for ll in baze\n",
    "                                DroneRoute[ll, :, uav[uu]] = DroneRoute[ll+1, :, uav[uu]]\n",
    "                            end\n",
    "                            DroneRoute[satr[end], :, uav[uu]] .= 0\n",
    "                        else\n",
    "                            flag = 1\n",
    "                            return Sol, flag\n",
    "                        end\n",
    "                    else\n",
    "                        baze = h:satr[end]-1\n",
    "                        for ll in baze\n",
    "                            DroneRoute[ll, :, uav[uu]] = DroneRoute[ll+1, :, uav[uu]]\n",
    "                        end\n",
    "                        DroneRoute[satr[end], :, uav[uu]] .= 0\n",
    "                    end\n",
    "                elseif h == 1\n",
    "                    baze = h:satr[end]-1\n",
    "                    for ll in baze\n",
    "                        DroneRoute[ll, :, uav[uu]] = DroneRoute[ll+1, :, uav[uu]]\n",
    "                    end\n",
    "                    DroneRoute[satr[end], :, uav[uu]] .= 0\n",
    "                end\n",
    "            elseif h == length(satr)\n",
    "                DroneRoute[h, :, uav[uu]] .= 0\n",
    "            end\n",
    "        else\n",
    "            DroneRoute[h, g:end-1, uav[uu]] = DroneRoute[h, g+1:end, uav[uu]]\n",
    "            DroneRoute[h, end, uav[uu]] = 0\n",
    "        end\n",
    "    end\n",
    "\n",
    "    truck = copy(TruckRoute)\n",
    "    for i in 1:tru\n",
    "        truck[i, count(!iszero, truck[i, :])] = 0\n",
    "    end\n",
    "    element = findall(!iszero, truck)\n",
    "    num = rand(1:length(element))\n",
    "    truck[element[num]]\n",
    "\n",
    "    sa_so_indices = findfirst(TruckRoute .== truck[element[num]])\n",
    "    sa, so = Tuple(sa_so_indices)\n",
    "    if length(sa) > 1\n",
    "        sa = sa[1]\n",
    "        so = so[1]\n",
    "    end\n",
    "    \n",
    "    TruckRoute = hcat(TruckRoute, zeros(tru, 1))\n",
    "    TruckRoute[sa, so+2:end] = TruckRoute[sa, so+1:end-1]\n",
    "    TruckRoute[sa, so+1] = Dronenode\n",
    "    TruckRoute=TruckRoute[:,1:end-1]\n",
    "\n",
    "    Sol[\"TruckRoute\"] = Int64.(TruckRoute)\n",
    "    Sol[\"DroneRoute\"] = Int64.(DroneRoute)\n",
    "    return Sol, flag\n",
    "end"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 27,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "Insertionewemtehanii (generic function with 1 method)"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "using Graphs\n",
    "\n",
    "function Insertionewemtehanii(model, Sol, flag)\n",
    "\n",
    "    nNodes = model[\"nNodes\"]\n",
    "    ua = model[\"uav\"]\n",
    "    tru = model[\"tru\"]\n",
    "    L = model[\"L\"]\n",
    "    Du = model[\"Du\"]\n",
    "    Dt = model[\"Dt\"]\n",
    "\n",
    "    TruckRoute = Sol[\"TruckRoute\"]\n",
    "    DroneRoute = Sol[\"DroneRoute\"]\n",
    "    flag = 0\n",
    "\n",
    "    truckroute = copy(TruckRoute)  # all truckroutes excep landing and launching nodes and depot \n",
    "    truckroute2 = copy(TruckRoute)  # all truckroutes excep landing and launching nodes and depot \n",
    "\n",
    "    for i in 1:tru\n",
    "        truckroute[i, count(!iszero, truckroute[i, :])] = 0\n",
    "    end\n",
    "    truckroute[:, 1] .= 0  # initial depot is eliminated \n",
    "\n",
    "    uav = []\n",
    "    uf = []  #uavs with no flight\n",
    "    \n",
    "    #added:\n",
    "    for u in 1:ua\n",
    "        satr = findall(!iszero, DroneRoute[:, 1, u])  # number of flights\n",
    "        if !isempty(satr)  # if the uav has flight\n",
    "            push!(uav, u)  # choose uavs with flight \n",
    "            for i in 1:length(satr)\n",
    "                aa_bb = findfirst(x -> x == DroneRoute[satr[i], 1, u], truckroute)\n",
    "\n",
    "                if !isnothing(aa_bb)\n",
    "                    aa, bb = Tuple(aa_bb)\n",
    "                    truckroute[aa, bb] = 0  # launch node eliminated \n",
    "                end\n",
    "                \n",
    "                cc_dd = findfirst(x -> x == DroneRoute[satr[i], count(!iszero, DroneRoute[satr[i], :, u]), u], truckroute)\n",
    "                if !isnothing(cc_dd)\n",
    "                    \n",
    "                    cc, dd = Tuple(cc_dd)\n",
    "                    truckroute[cc, dd] = 0  # land node eliminated\n",
    "                end\n",
    "                                                \n",
    "            end\n",
    "        else\n",
    "            push!(uf, u)\n",
    "        end\n",
    "    end\n",
    "\n",
    "    if isempty(findall(!iszero, truckroute))\n",
    "        flag = 1\n",
    "        return Sol, flag\n",
    "    end\n",
    "    \n",
    "    truckroute = filter(x -> x != 0, truckroute) \n",
    "    n = rand(1:length(truckroute))  # Randomly select a node from truckroute\n",
    "    node1 = truckroute[n]  # truck node to be inserted\n",
    "\n",
    "    row_col = findfirst(x -> x == node1, TruckRoute)\n",
    "    row, column = Tuple(row_col)\n",
    "\n",
    "    # Distance matrix\n",
    "    distance = zeros(nNodes, nNodes)\n",
    "    for i in 1:nNodes\n",
    "        if !isempty(findall(x -> x == i, TruckRoute))  # node in truckroute\n",
    "            \n",
    "            if i == 1\n",
    "                for t in 1:tru\n",
    "                    distance[i, TruckRoute[t, 1+1]] = 1\n",
    "                end\n",
    "            else\n",
    "                fa_ja = findfirst(x -> x == i, TruckRoute)\n",
    "                fa,ja=Tuple(fa_ja)\n",
    "                if fa !== nothing && ja !== nothing\n",
    "                    distance[i, TruckRoute[fa, ja+1]] = 1\n",
    "                end\n",
    "            end\n",
    "        end\n",
    "        \n",
    "        #added:\n",
    "        ind = findall(x -> x == i, DroneRoute)\n",
    "        if !isempty(ind)  # node in DroneRoute            \n",
    "            tuples = [Tuple(CartesianIndices(DroneRoute)[ik]) for ik in ind]\n",
    "            q1 = getindex.(tuples, 1)\n",
    "            q2 = getindex.(tuples, 2)\n",
    "            q3 = getindex.(tuples, 3)\n",
    "            for u in 1:length(q3)\n",
    "                if q2[u] != count(!iszero, DroneRoute[q1[u], :, q3[u]])\n",
    "                    distance[i, DroneRoute[q1[u], q2[u]+1, q3[u]]] = 2\n",
    "                end\n",
    "            end\n",
    "        end\n",
    "\n",
    "    end\n",
    "    \n",
    "    graphic = Graphs.DiGraph(distance)\n",
    "\n",
    "    uu = []  # available uavs\n",
    "    multi = zeros(nNodes, ua)  # (flight, uav) => if we have a uav, which of its flights can be multivist? \n",
    "    addflight = zeros(nNodes, ua)  # (flight, uav) => which uavs can be added new flights?\n",
    "    for u in 1:ua\n",
    "        \n",
    "        #added:\n",
    "        parvaz = findall(!iszero, DroneRoute[:, 1, u])\n",
    "        if !isempty(parvaz)  # if it owed flight before\n",
    "            # Check multivisit            \n",
    "            for i in 1:length(parvaz)\n",
    "                place = DroneRoute[parvaz[i], 1:count(!iszero, DroneRoute[parvaz[i], :, u]), u]\n",
    "                for z in 1:length(place)-1\n",
    "                    flight = Du[place[z], node1] + Du[node1, place[z+1]]\n",
    "                    for k in 1:length(place)-1\n",
    "                        if z != k                            \n",
    "                            flight += Du[place[k], place[k+1]] \n",
    "                        end\n",
    "                    end\n",
    "                    if flight <= L  # alter it to be multivisit if we can\n",
    "                        multi[parvaz[i], u] = 1\n",
    "                        push!(uu, u)\n",
    "                    end\n",
    "                end\n",
    "            end\n",
    "            if DroneRoute[parvaz[end], count(!iszero, DroneRoute[parvaz[end], :, u]), u] != 1  #if the last flight of the uav is not returned to 1 \n",
    "                addflight[parvaz[end]+1, u] = 1\n",
    "                push!(uu, u)\n",
    "            end\n",
    "        else  # if uav didnt have flight before \n",
    "            addflight[1, u] = 1  #the first row of uav route is filled(gets  flight) \n",
    "            push!(uu, u)\n",
    "        end\n",
    "    end\n",
    "\n",
    "    uu = unique(uu)\n",
    "\n",
    "    if isempty(uu)\n",
    "        flag = 1\n",
    "        return Sol, flag\n",
    "    end\n",
    "\n",
    "    dr = rand(1:length(uu))  # Randomly select a drone\n",
    "    drone = uu[dr]  # drone which we want to insert a node to \n",
    "\n",
    "    u = drone       \n",
    "    if !isempty(findall(x -> x == 1, multi[:, drone])) && !isempty(findall(x -> x == 1, addflight[:, u]))  # if we are able to make the drone multivisit and also give it a new flight\n",
    "        a = rand()\n",
    "        if a > 0.5\n",
    "            # multivisit\n",
    "            satr = findall(x -> x == 1, multi[:, drone]) \n",
    "            if length(satr) > 1  # if we could add flights to a number of rows \n",
    "                f = rand(1:length(satr))  # choose one row randomly\n",
    "                location = DroneRoute[satr[f], 1:count(!iszero, DroneRoute[satr[f], :, drone]), drone]\n",
    "                for ii in 1:length(location)-1\n",
    "                    flightrange = Du[location[ii], node1] + Du[node1, location[ii+1]]\n",
    "                    for jj in 1:length(location)-1\n",
    "                        if jj != ii\n",
    "                            flightrange += Du[location[jj], location[jj+1]]\n",
    "                        end\n",
    "                    end\n",
    "                    if flightrange <= L\n",
    "                        adress = findfirst(x -> x == location[ii], DroneRoute[satr[f], :, drone])\n",
    "                        DroneRoute[satr[f], adress+2:end, drone] = DroneRoute[satr[f], adress+1:end-1, drone]                        \n",
    "                        DroneRoute[satr[f], adress+1, drone] = node1                        \n",
    "                        TruckRoute[row, column:end-1] = TruckRoute[row, column+1:end]\n",
    "                        TruckRoute[row,end]=0                        \n",
    "                        \n",
    "                        break\n",
    "                    end\n",
    "                end\n",
    "            elseif length(satr) == 1  # if we could add a flight to one \n",
    "                location = DroneRoute[satr, 1:count(!iszero, DroneRoute[satr, :, drone]), drone]\n",
    "                for ii in 1:length(location)-1\n",
    "                    flightrange = Du[location[ii], node1] + Du[node1, location[ii+1]]\n",
    "                    for jj in 1:length(location)-1\n",
    "                        if jj != ii\n",
    "                            flightrange += Du[location[jj], location[jj+1]]\n",
    "                        end\n",
    "                    end\n",
    "                    if flightrange <= L                        \n",
    "                        adress = findfirst(x -> x == location[ii], DroneRoute[satr, :, drone])\n",
    "                        \n",
    "                        adress = Tuple(adress)[1]\n",
    "                        DroneRoute[first(satr), first(adress)+2:end, drone] = DroneRoute[first(satr), first(adress)+1:end-1, drone]\n",
    "                        DroneRoute[satr, first(adress) .+1, first(drone)] .= node1                        \n",
    "                        TruckRoute[row, column:end-1] = TruckRoute[row, column+1:end]\n",
    "                        TruckRoute[row,end]=0                    \n",
    "                        break\n",
    "                    end\n",
    "                end\n",
    "            end\n",
    "        elseif a < 0.5\n",
    "            \n",
    "            satr = findall(x -> x == 1, addflight[:, u])\n",
    "            satr = first(satr) \n",
    "            saa_soo = findfirst(x -> x == DroneRoute[satr-1, count(!iszero, DroneRoute[satr-1, :, u]), u], TruckRoute)\n",
    "            saa, soo = Tuple(saa_soo)\n",
    "            launch = TruckRoute[saa, soo:count(!iszero, TruckRoute[saa, :])]\n",
    "            launch = filter(x -> x != node1 && x != 0 && x != 1, launch)\n",
    "            la = rand(1:length(launch))\n",
    "            launching = launch[la]\n",
    "            land = filter(x -> x != launching && x != node1 && x != 0, TruckRoute)\n",
    "            include = []\n",
    "            for l in land\n",
    "                flight = Du[launching, node1] + Du[node1, l]\n",
    "                if flight <= L\n",
    "                    push!(include, l)\n",
    "                end\n",
    "            end\n",
    "            if isempty(include)\n",
    "                flag = 1\n",
    "                return Sol, flag\n",
    "            end\n",
    "            # allpath:\n",
    "            s = all_simple_paths(graphic, 1, launching)\n",
    "            aa = vcat(s...)\n",
    "            for jj in 1:length(aa)\n",
    "                include = filter(x -> x != aa[jj], include)\n",
    "            end\n",
    "            if launching != 1  #if the launch is not 1(depot), the land can be 1(depot).\n",
    "                r = Du[launching, node1] + Du[node1, 1]\n",
    "                if r <= L\n",
    "                    push!(include, 1)\n",
    "                end\n",
    "            end\n",
    "            if isempty(include)  #if no landing alternative exist\n",
    "                flag = 1\n",
    "                return Sol, flag\n",
    "            end\n",
    "            no = rand(1:length(include))\n",
    "            DroneRoute[satr, 1, u] = launching\n",
    "            DroneRoute[satr, 2, u] = node1\n",
    "            DroneRoute[satr, 3, u] = include[no]\n",
    "            \n",
    "            TruckRoute[row, column:end-1] = TruckRoute[row, column+1:end]\n",
    "            TruckRoute[row,end]=0            \n",
    "        end\n",
    "    elseif !isempty(findall(x -> x == 1, multi[:, drone])) && isempty(findall(x -> x == 1, addflight[:, u]))  #if we can only change the drone to be multivisit and we can not add a new flight to it \n",
    "        # multivisit only\n",
    "        satr = findall(x -> x == 1, multi[:, drone])\n",
    "        if length(satr) > 1  # if we can add flight to the number of rows \n",
    "            f = rand(1:length(satr))  # choose a row randomly \n",
    "            location = DroneRoute[satr[f], 1:count(!iszero, DroneRoute[satr[f], :, drone]), drone]\n",
    "            for ii in 1:length(location)-1\n",
    "                flightrange = Du[location[ii], node1] + Du[node1, location[ii+1]]\n",
    "                for jj in 1:length(location)-1\n",
    "                    if jj != ii\n",
    "                        flightrange += Du[location[jj], location[jj+1]]\n",
    "                    end\n",
    "                end\n",
    "                if flightrange <= L\n",
    "                    adress = findfirst(x -> x == location[ii], DroneRoute[satr[f], :, drone])\n",
    "                    DroneRoute[satr[f], adress+2:end, drone] = DroneRoute[satr[f], adress+1:end-1, drone]\n",
    "                    DroneRoute[satr[f], adress+1, drone] = node1\n",
    "                    \n",
    "                    TruckRoute[row, column:end-1] = TruckRoute[row, column+1:end]\n",
    "                    TruckRoute[row,end]=0\n",
    "                    \n",
    "                    break\n",
    "                end\n",
    "            end\n",
    "        elseif length(satr) == 1  # if we can add a flight to one row\n",
    "            location = DroneRoute[satr, 1:count(!iszero, DroneRoute[satr, :, drone]), drone]\n",
    "            for ii in 1:length(location)-1\n",
    "                flightrange = Du[location[ii], node1] + Du[node1, location[ii+1]]\n",
    "                for jj in 1:length(location)-1\n",
    "                    if jj != ii\n",
    "                        flightrange += Du[location[jj], location[jj+1]]\n",
    "                    end\n",
    "                end\n",
    "                if flightrange <= L\n",
    "                    adress = findfirst(x -> x == location[ii], DroneRoute[satr, :, drone])\n",
    "                    adress = Tuple(adress)[1]\n",
    "                    \n",
    "                    DroneRoute[first(satr), first(adress)+2:end, drone] = DroneRoute[first(satr), first(adress)+1:end-1, drone]\n",
    "                    DroneRoute[first(satr), first(adress)+1, drone] = node1\n",
    "                    \n",
    "                    TruckRoute[row, column:end-1] = TruckRoute[row, column+1:end]\n",
    "                    TruckRoute[row,end]=0\n",
    "                    \n",
    "                    break\n",
    "                end\n",
    "            end\n",
    "        end\n",
    "    elseif isempty(findall(x -> x == 1, multi[:, drone])) && !isempty(findall(x -> x == 1, addflight[:, u]))  # if we cant change it to be multivisit but we can add a flight to it\n",
    "        satr = findall(x -> x == 1, addflight[:, u])\n",
    "        if first(satr) == 1  # if we can add a flight to the first row \n",
    "            launch = filter(x -> x != node1 && x != 0 && x != 1, TruckRoute)\n",
    "            push!(launch, 1)\n",
    "            laun = rand(1:length(launch))\n",
    "            launching = launch[laun]\n",
    "\n",
    "            land = filter(x -> x != launching && x != node1 && x != 0, TruckRoute)\n",
    "            push!(land, 1)\n",
    "            include = []\n",
    "            for l in land\n",
    "                flight = Du[launching, node1] + Du[node1, l]\n",
    "                if flight <= L\n",
    "                    push!(include, l)\n",
    "                end\n",
    "            end\n",
    "            # allpath:\n",
    "            s = all_simple_paths(graphic, 1, launching)\n",
    "            aa = vcat(s...)\n",
    "            for jj in 1:length(aa)\n",
    "                include = filter(x -> x != aa[jj], include)\n",
    "            end\n",
    "            if launching != 1  # if the launch is not 1 , the landing node can be 1\n",
    "                rr = Du[launching, node1] + Du[node1, 1]\n",
    "                if rr <= L\n",
    "                    push!(include, 1)\n",
    "                end\n",
    "            end\n",
    "            if isempty(include)  # if no landing alternative exist \n",
    "                flag = 1\n",
    "                return Sol, flag\n",
    "            end\n",
    "            no = rand(1:length(include))\n",
    "            DroneRoute[satr, 1, u] .= launching\n",
    "            DroneRoute[satr, 2, u] .= node1\n",
    "            DroneRoute[satr, 3, u] .= include[no]\n",
    "            \n",
    "            TruckRoute[row, column:end-1] = TruckRoute[row, column+1:end]\n",
    "            TruckRoute[row,end]=0\n",
    "            \n",
    "        else  # if we are adding a flight, we must find the landing of the previous flight and launch from the nodes afterwards\n",
    "            saa_soo = findfirst(x -> x == DroneRoute[first(satr)-1, count(!iszero, DroneRoute[first(satr)-1, :, u]), u], TruckRoute)\n",
    "            saa, soo = Tuple(saa_soo)\n",
    "            launch = TruckRoute[saa, soo:count(!iszero, TruckRoute[saa, :])]\n",
    "            launch = filter(x -> x != node1 && x != 0 && x != 1, launch)\n",
    "            la = rand(1:length(launch))\n",
    "            launching = launch[la]\n",
    "            land = filter(x -> x != node1 && x != 0 && x != launching, TruckRoute)\n",
    "            include = []\n",
    "            for l in land\n",
    "                flight = Du[launching, node1] + Du[node1, l]\n",
    "                if flight <= L\n",
    "                    push!(include, l)\n",
    "                end\n",
    "            end\n",
    "            if launching != 1  #if the launch is not 1 , landing node can be 1\n",
    "                r = Du[launching, node1] + Du[node1, 1]\n",
    "                if r <= L\n",
    "                    push!(include, 1)\n",
    "                end\n",
    "            end\n",
    "            if isempty(include)\n",
    "                flag = 1\n",
    "                return Sol, flag\n",
    "            end\n",
    "            # allpath: launching\n",
    "            s = all_simple_paths(graphic, 1, launching)\n",
    "            aa = vcat(s...)\n",
    "            for jj in 1:length(aa)\n",
    "                include = filter(x -> x != aa[jj], include)\n",
    "            end\n",
    "            if isempty(include)  # if no landing alternative exist \n",
    "                flag = 1\n",
    "                return Sol, flag\n",
    "            end\n",
    "            no = rand(1:length(include))\n",
    "            DroneRoute[satr, 1, u] .= launching\n",
    "            DroneRoute[satr, 2, u] .= node1\n",
    "            DroneRoute[satr, 3, u] .= include[no]\n",
    "            \n",
    "            TruckRoute[row, column:end-1] = TruckRoute[row, column+1:end]\n",
    "            TruckRoute[row,end]=0\n",
    "            \n",
    "        end\n",
    "    end\n",
    "\n",
    "    \n",
    "    \n",
    "    indices=findall(x -> x==1,TruckRoute)\n",
    "    indices=Tuple(indices)\n",
    "        \n",
    "\n",
    "    \n",
    "    Sol[\"TruckRoute\"] = Int64.(TruckRoute)\n",
    "    Sol[\"DroneRoute\"] = Int64.(DroneRoute)\n",
    "    return Sol, flag\n",
    "end"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 28,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "launchange1 (generic function with 1 method)"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "#using LightGraphs\n",
    "\n",
    "function launchange1(model, Sol, flag)\n",
    "\n",
    "    nNodes = model[\"nNodes\"]\n",
    "    ua = model[\"uav\"]\n",
    "    tru = model[\"tru\"]\n",
    "    L = model[\"L\"]\n",
    "    Du = model[\"Du\"]\n",
    "    Dt = model[\"Dt\"]\n",
    "\n",
    "    TruckRoute = Sol[\"TruckRoute\"]\n",
    "    DroneRoute = Sol[\"DroneRoute\"]\n",
    "    flag = 0\n",
    "\n",
    "\n",
    "    uav = []\n",
    "    for u in 1:ua\n",
    "        if !isempty(findall(!iszero, DroneRoute[:, :, u]))  # if we have flight\n",
    "            push!(uav, u)\n",
    "        end\n",
    "    end\n",
    "    if isempty(uav)  #if we had no uav\n",
    "        flag = 1\n",
    "        return Sol, flag\n",
    "    end\n",
    "\n",
    "    if count(!iszero, TruckRoute) == 4  # if it was only one \n",
    "        flag = 1\n",
    "        return Sol, flag\n",
    "    end\n",
    "\n",
    "    pahpad = rand(1:length(uav))  # choosing a drone randomly\n",
    "    parvaz = findall(!iszero, DroneRoute[:, 1, uav[pahpad]])  # choosing a flight randomly\n",
    "    par = rand(1:length(parvaz))  # the flight which we must alter its launching node \n",
    "\n",
    "    launch = []\n",
    "    if (length(parvaz) == 1) || ((length(parvaz) != 1) && (parvaz[par] == 1))  # if we have 1 flight, or if we have number of flights, but we are in the first row \n",
    "        satr_sotoon = findfirst(x -> x == DroneRoute[parvaz[par], count(!iszero, DroneRoute[parvaz[par], :, uav[pahpad]]), uav[pahpad]], TruckRoute)\n",
    "        satr, sotoon = Tuple(satr_sotoon)\n",
    "        if length(satr) > 1\n",
    "            satr = satr[1]\n",
    "            sotoon = sotoon[1]\n",
    "        end\n",
    "        launch = vcat(launch, TruckRoute[satr, 1:sotoon-1])\n",
    "        for i in 1:tru\n",
    "            if i == satr\n",
    "                continue\n",
    "            else\n",
    "                launch = vcat(launch, TruckRoute[i, :])\n",
    "            end\n",
    "        end\n",
    "        launch = filter(x -> x != 0, launch)\n",
    "        if isempty(launch)\n",
    "            flag = 1\n",
    "            return Sol, flag\n",
    "        end\n",
    "\n",
    "        cost = DroneRoute[parvaz[par], 1:count(!iszero, DroneRoute[parvaz[par], :, uav[pahpad]]), uav[pahpad]]\n",
    "        cost = cost[2:end]\n",
    "        hazine = 0\n",
    "        for j in 1:length(cost)-1\n",
    "            hazine += Du[cost[j], cost[j+1]]\n",
    "        end\n",
    "\n",
    "        include = []  # nodes which their flights have no problem \n",
    "        for i in launch\n",
    "            flightrange = hazine + Du[cost[1], i]\n",
    "            if flightrange <= L\n",
    "                push!(include, i)\n",
    "            end\n",
    "        end\n",
    "\n",
    "        if isempty(include)\n",
    "            flag = 1\n",
    "            return Sol, flag\n",
    "        end\n",
    "\n",
    "        distance = zeros(nNodes, nNodes)\n",
    "        for i in 1:nNodes\n",
    "            if !isempty(findall(x -> x == i, TruckRoute))  # node in truck route\n",
    "                \n",
    "                if i == 1\n",
    "                    for t in 1:tru\n",
    "                        distance[i, TruckRoute[t, 1+1]] = 1\n",
    "                    end\n",
    "                else\n",
    "                    fa_ja = findfirst(x -> x == i, TruckRoute)\n",
    "                    fa,ja=Tuple(fa_ja)\n",
    "                    if fa !== nothing && ja !== nothing\n",
    "                        distance[i, TruckRoute[fa, ja+1]] = 1\n",
    "                    end\n",
    "                end\n",
    "            end\n",
    "           \n",
    "            #added:\n",
    "            ind = findall(x -> x == i, DroneRoute)\n",
    "            if !isempty(ind)  # node in DroneRoute                \n",
    "                tuples = [Tuple(CartesianIndices(DroneRoute)[ik]) for ik in ind]\n",
    "                q1 = getindex.(tuples, 1)\n",
    "                q2 = getindex.(tuples, 2)\n",
    "                q3 = getindex.(tuples, 3)\n",
    "                for u in 1:length(q3)\n",
    "                    if q2[u] != count(!iszero, DroneRoute[q1[u], :, q3[u]])\n",
    "                        distance[i, DroneRoute[q1[u], q2[u]+1, q3[u]]] = 2\n",
    "                    end\n",
    "                end\n",
    "            end\n",
    "        end\n",
    "\n",
    "        graphic = Graphs.DiGraph(distance)\n",
    "        s = all_simple_paths(graphic, DroneRoute[parvaz[par], count(!iszero, DroneRoute[parvaz[par], :, uav[pahpad]]), uav[pahpad]], 1)\n",
    "        aa = vcat(s...)\n",
    "        for jj in 1:length(aa)\n",
    "            include = filter(x -> x != aa[jj], include)\n",
    "        end\n",
    "\n",
    "        if DroneRoute[parvaz[par], count(!iszero, DroneRoute[parvaz[par], :, uav[pahpad]]), uav[pahpad]] != 1  #if the landing is not 1,the laundhing node can be 1\n",
    "            r = hazine + Du[cost[1], 1]\n",
    "            if r <= L\n",
    "                push!(include, 1)\n",
    "            end\n",
    "        end\n",
    "\n",
    "        if isempty(include)\n",
    "            flag = 1\n",
    "            return Sol, flag\n",
    "        end\n",
    "    elseif (length(parvaz) != 1) && (parvaz[par] != 1)  # if we have number of flights and we are not in the first row\n",
    "        satr_sotoon = findfirst(x -> x == DroneRoute[parvaz[par], count(!iszero, DroneRoute[parvaz[par], :, uav[pahpad]]), uav[pahpad]], TruckRoute)\n",
    "        satr, sotoon = Tuple(satr_sotoon)\n",
    "        if length(satr) > 1  # if it is launched from 1 \n",
    "            satr = satr[1]\n",
    "            sotoon = sotoon[1]\n",
    "        end\n",
    "        if count(!iszero, DroneRoute[parvaz[par]-1, :, uav[pahpad]])==0\n",
    "            flag=1\n",
    "            return Sol, flag\n",
    "        end\n",
    "        sat_sot = findfirst(x -> x == DroneRoute[parvaz[par]-1, count(!iszero, DroneRoute[parvaz[par]-1, :, uav[pahpad]]), uav[pahpad]], TruckRoute)\n",
    "        sat, sot = Tuple(sat_sot)\n",
    "\n",
    "        if satr == sat\n",
    "            launch = vcat(launch, TruckRoute[sat, sot:sotoon-1])\n",
    "        else  #if the landing node of the current flight, and the landing node of the previous flight is not on the same flight\n",
    "            launch = vcat(launch, TruckRoute[sat, sot:end])\n",
    "        end\n",
    "        launch = filter(x -> x != 0 && x != 1, launch)\n",
    "\n",
    "        if isempty(launch)\n",
    "            flag = 1\n",
    "            return Sol, flag\n",
    "        end\n",
    "\n",
    "        cost = DroneRoute[parvaz[par], 1:count(!iszero, DroneRoute[parvaz[par], :, uav[pahpad]]), uav[pahpad]]\n",
    "        cost = cost[2:end]\n",
    "        hazine = 0\n",
    "        for j in 1:length(cost)-1\n",
    "            hazine += Du[cost[j], cost[j+1]]\n",
    "        end\n",
    "\n",
    "        include = []\n",
    "        for i in launch\n",
    "            flightrange = hazine + Du[cost[1], i]\n",
    "            if flightrange <= L\n",
    "                push!(include, i)\n",
    "            end\n",
    "        end\n",
    "\n",
    "        if isempty(include)\n",
    "            flag = 1\n",
    "            return Sol, flag\n",
    "        end\n",
    "\n",
    "        distance = zeros(nNodes, nNodes)\n",
    "        for i in 1:nNodes\n",
    "            if !isempty(findall(x -> x == i, TruckRoute))  # node in truck route\n",
    "                \n",
    "                if i == 1\n",
    "                    for t in 1:tru\n",
    "                        distance[i, TruckRoute[t, 1+1]] = 1\n",
    "                    end\n",
    "                else\n",
    "                    fa_ja = findfirst(x -> x == i, TruckRoute)\n",
    "                    fa,ja=Tuple(fa_ja)\n",
    "                    if fa !== nothing && ja !== nothing\n",
    "                        distance[i, TruckRoute[fa, ja+1]] = 1\n",
    "                    end\n",
    "                end\n",
    "\n",
    "            end\n",
    "            \n",
    "            #added:\n",
    "            ind = findall(x -> x == i, DroneRoute)\n",
    "            if !isempty(ind)  # node in DroneRoute                \n",
    "                tuples = [Tuple(CartesianIndices(DroneRoute)[ik]) for ik in ind]\n",
    "                q1 = getindex.(tuples, 1)\n",
    "                q2 = getindex.(tuples, 2)\n",
    "                q3 = getindex.(tuples, 3)\n",
    "                for u in 1:length(q3)\n",
    "                    if q2[u] != count(!iszero, DroneRoute[q1[u], :, q3[u]])\n",
    "                        distance[i, DroneRoute[q1[u], q2[u]+1, q3[u]]] = 2\n",
    "                    end\n",
    "                end\n",
    "            end\n",
    "            \n",
    "        end\n",
    "\n",
    "        graphic = Graphs.DiGraph(distance)\n",
    "        s = all_simple_paths(graphic, DroneRoute[parvaz[par], count(!iszero, DroneRoute[parvaz[par], :, uav[pahpad]]), uav[pahpad]], 1)\n",
    "        aa = vcat(s...)\n",
    "        for jj in 1:length(aa)\n",
    "            include = filter(x -> x != aa[jj], include)\n",
    "        end\n",
    "\n",
    "        if isempty(include)\n",
    "            flag = 1\n",
    "            return Sol, flag\n",
    "        end\n",
    "    end\n",
    "\n",
    "    include = filter(x -> x != DroneRoute[parvaz[par], 1, uav[pahpad]], include)  # the previous launching node is eliminated\n",
    "\n",
    "    if isempty(include)\n",
    "        flag = 1\n",
    "        return Sol, flag\n",
    "    end\n",
    "\n",
    "    no = rand(1:length(include))\n",
    "    DroneRoute[parvaz[par], 1, uav[pahpad]] = include[no]\n",
    "\n",
    "    \n",
    "\n",
    "    Sol[\"TruckRoute\"] = TruckRoute\n",
    "    Sol[\"DroneRoute\"] = DroneRoute\n",
    "    return Sol, flag\n",
    "end"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 29,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "CoolingRate (generic function with 1 method)"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "using LinearAlgebra\n",
    "\n",
    "function CoolingRate(Current, PRIM)\n",
    "    Peak = [1.025, 0.9, 0.8]  # Define the Peak vector\n",
    "    BestSol = Current  # Assign Current to BestSol\n",
    "\n",
    "    # Calculate DELTA_C - element-wise operations\n",
    "    DELTA_C = (BestSol .- PRIM[\"Cost\"]) ./ BestSol\n",
    "    alpha = 1 .- 0.99 .* exp.(-5 .* DELTA_C.^2)  # Note the dots for element-wise operations\n",
    "    DELTA_C = DELTA_C ./ alpha\n",
    "\n",
    "    # Initialize variables\n",
    "    better = 0.0\n",
    "    nochange = 0.0\n",
    "    worse = 0.0\n",
    "\n",
    "    # Determine the values of worse, nochange, and better based on DELTA_C\n",
    "    # Note: DELTA_C is now a vector, so we need to handle each element\n",
    "    # This assumes DELTA_C is a single-element vector - if not, you'll need to decide how to handle it\n",
    "    delta = DELTA_C[1]  # Taking first element if it's a single-element vector\n",
    "    \n",
    "    if delta <= -1\n",
    "        worse = 1.0\n",
    "    elseif delta > -1 && delta <= 0\n",
    "        worse = -delta\n",
    "        nochange = 1 + delta\n",
    "    elseif delta > 0 && delta <= 1\n",
    "        nochange = 1 - delta\n",
    "        better = delta\n",
    "    elseif delta > 1\n",
    "        better = 1.0\n",
    "    end\n",
    "\n",
    "    # Create the Mu vector\n",
    "    Mu = [worse; nochange; better]\n",
    "\n",
    "    # Calculate CR as the dot product of Peak and Mu\n",
    "    CR = dot(Peak, Mu)\n",
    "\n",
    "    return CR\n",
    "end\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 30,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "landchange4 (generic function with 1 method)"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "function landchange4(model, Sol, flag)\n",
    "    ua = model[\"uav\"]\n",
    "    L = model[\"L\"]\n",
    "    Du = model[\"Du\"]\n",
    "    tru = model[\"tru\"]\n",
    "    nNodes = model[\"nNodes\"]\n",
    "    TruckRoute = Sol[\"TruckRoute\"]\n",
    "    DroneRoute = Sol[\"DroneRoute\"]\n",
    "    flag = 0\n",
    "\n",
    "    uav = []\n",
    "    for u in 1:ua\n",
    "        if !isempty(findall(!iszero, DroneRoute[:, :, u])) # if the drone has flights\n",
    "            push!(uav, u)\n",
    "        end\n",
    "    end\n",
    "    if isempty(uav) # if no drones have flights, no changes\n",
    "        flag = 1\n",
    "        return Sol, flag\n",
    "    end\n",
    "\n",
    "    if count(!iszero, TruckRoute) == 4 # if only one truck route\n",
    "        flag = 1\n",
    "        return Sol, flag\n",
    "    end\n",
    "\n",
    "    pahpad = rand(1:length(uav)) # choose a drone randomly\n",
    "\n",
    "    parvaz = findall(!iszero, DroneRoute[:, 1, uav[pahpad]]) # choose a flight randomly\n",
    "    par = rand(1:length(parvaz)) # choose a flight to modify\n",
    "\n",
    "\n",
    "    land = []\n",
    "    if parvaz[par] == length(parvaz) # if it's the last flight\n",
    "        # Find launch node of the selected flight\n",
    "        ad_res = findfirst(x -> x == DroneRoute[parvaz[par], 1, uav[pahpad]], TruckRoute)\n",
    "        ad,res=Tuple(ad_res)\n",
    "        if ad !== nothing && res !== nothing\n",
    "            if length(ad) > 1\n",
    "                ad = ad[1]\n",
    "                res = res[1]\n",
    "            end\n",
    "        end\n",
    "\n",
    "        #Find landing node of the selected flight\n",
    "        adr_ress = findfirst(x -> x == DroneRoute[parvaz[par], count(!iszero, DroneRoute[parvaz[par], :, uav[pahpad]]), uav[pahpad]], TruckRoute)\n",
    "        adr,ress=Tuple(adr_ress)\n",
    "        if adr !== nothing && ress !== nothing\n",
    "            if length(adr) > 1\n",
    "                adr = adr[1]\n",
    "                ress = ress[1]\n",
    "            end\n",
    "        end\n",
    "\n",
    "\n",
    "        # Extract landing alternatives\n",
    "        land = vcat(land, TruckRoute[ad, res+1:count(!iszero, TruckRoute[ad, :])])\n",
    "        filter!(x -> x != TruckRoute[adr, ress], land)\n",
    "\n",
    "        if ad == 1\n",
    "            # Add remaining truck routes\n",
    "            chatgpt = TruckRoute[ad+1:tru, :]\n",
    "            cr, oss = size(chatgpt)\n",
    "            if ndims(land) == 1\n",
    "                CHATGPT = reshape(chatgpt, 1, cr*oss)\n",
    "                CHATGPT = CHATGPT[CHATGPT .!= 0]\n",
    "                land = vcat(land, CHATGPT)\n",
    "            else\n",
    "                CHATGPT = reshape(chatgpt, size(land, 1), size(land, 2))\n",
    "                land = vcat(land, CHATGPT)\n",
    "            end\n",
    "        else\n",
    "            for i in 1:tru\n",
    "                if i == ad\n",
    "                    continue\n",
    "                else\n",
    "                    land = vcat(land, TruckRoute[i, :])\n",
    "                end\n",
    "            end\n",
    "        end        \n",
    "\n",
    "        filter!(x -> x != 0, land)\n",
    "        if isempty(land)\n",
    "            flag = 1\n",
    "            return Sol, flag\n",
    "        end\n",
    "\n",
    "        # Calculate flight range\n",
    "        cost = DroneRoute[parvaz[par], 1:count(!iszero, DroneRoute[parvaz[par], :, uav[pahpad]]), uav[pahpad]]\n",
    "        pop!(cost)\n",
    "        hazine = sum(Du[cost[j], cost[j+1]] for j in 1:length(cost)-1)\n",
    "        include = []\n",
    "        for i in land\n",
    "            flightrange = hazine + Du[cost[end], i]\n",
    "            if flightrange <= L\n",
    "                push!(include, i)\n",
    "            end\n",
    "        end\n",
    "        if isempty(include)\n",
    "            flag = 1\n",
    "            return Sol, flag\n",
    "        end        \n",
    "\n",
    "        # Check all paths\n",
    "        distance = zeros(nNodes, nNodes)\n",
    "        for i in 1:nNodes\n",
    "           \n",
    "\n",
    "            if !isempty(findall(x -> x == i, TruckRoute)) # node in truck route\n",
    "                if i == 1\n",
    "                    for t in 1:tru\n",
    "                        distance[i, TruckRoute[t, 1+1]] = 1\n",
    "                    end\n",
    "                else\n",
    "                    fa_ja = findfirst(x -> x == i, TruckRoute)\n",
    "                    fa,ja=Tuple(fa_ja)\n",
    "                    if fa !== nothing && ja !== nothing\n",
    "                        distance[i, TruckRoute[fa, ja+1]] = 1\n",
    "                    end\n",
    "                end\n",
    "            end\n",
    "            \n",
    "            #added:\n",
    "            ind = findall(x -> x == i, DroneRoute)\n",
    "            if !isempty(ind) # node in drone route                \n",
    "                tuples = [Tuple(CartesianIndices(DroneRoute)[ik]) for ik in ind]\n",
    "                q1 = getindex.(tuples, 1)\n",
    "                q2 = getindex.(tuples, 2)\n",
    "                q3 = getindex.(tuples, 3)\n",
    "                for u in 1:length(q3)\n",
    "                    if q2[u] != count(!iszero, DroneRoute[q1[u], :, q3[u]])\n",
    "                        distance[i, DroneRoute[q1[u], q2[u]+1, q3[u]]] = 2\n",
    "                    end\n",
    "                end\n",
    "            end\n",
    "        end\n",
    "        \n",
    "        graphic = Graphs.DiGraph(distance)\n",
    "        s = collect(all_simple_paths(graphic, 1, DroneRoute[parvaz[par], 1, uav[pahpad]]))\n",
    "        aa = []\n",
    "        \n",
    "        for ii in 1:length(s)\n",
    "            aa = vcat(aa, s[ii])\n",
    "        end\n",
    "        \n",
    "\n",
    "        for jj in 1:length(aa)\n",
    "            filter!(x -> x != aa[jj], include)\n",
    "        end        \n",
    "\n",
    "        if DroneRoute[parvaz[par], 1, uav[pahpad]] != 1\n",
    "            r = hazine + Du[cost[end], 1]\n",
    "            if r <= L\n",
    "                push!(include, 1)\n",
    "            end\n",
    "        end\n",
    "\n",
    "        if isempty(include)\n",
    "            flag = 1\n",
    "            return Sol, flag\n",
    "        end\n",
    "        \n",
    "\n",
    "    else # if not the last flight\n",
    "\n",
    "        ad_res = findfirst(x -> x == DroneRoute[parvaz[par], 1, uav[pahpad]], TruckRoute)\n",
    "        ad,res=Tuple(ad_res)\n",
    "        if ad !== nothing && res !== nothing\n",
    "            if length(ad) > 1\n",
    "                ad = ad[1]\n",
    "                res = res[1]\n",
    "            end\n",
    "        end\n",
    "\n",
    "        sat_sot = findfirst(x -> x == DroneRoute[parvaz[par]+1, 1, uav[pahpad]], TruckRoute)\n",
    "        sat,sot=Tuple(sat_sot)\n",
    "        if sat !== nothing && sot !== nothing\n",
    "            if sat == ad\n",
    "                land = vcat(land, TruckRoute[ad, res+1:sot])\n",
    "            else\n",
    "                land = vcat(land, TruckRoute[sat, 1:sot])\n",
    "            end\n",
    "        end\n",
    "\n",
    "        if DroneRoute[parvaz[par], 1, uav[pahpad]] == 1\n",
    "            filter!(x -> x != 1, land)\n",
    "        end\n",
    "        filter!(x -> x != 0, land)\n",
    "        if isempty(land)\n",
    "            flag = 1\n",
    "            return Sol, flag\n",
    "        end\n",
    "\n",
    "        \n",
    "\n",
    "        cost = DroneRoute[parvaz[par], 1:count(!iszero, DroneRoute[parvaz[par], :, uav[pahpad]]), uav[pahpad]]\n",
    "        pop!(cost)\n",
    "        hazine = sum(Du[cost[j], cost[j+1]] for j in 1:length(cost)-1)\n",
    "        include = []\n",
    "        for i in land\n",
    "            flightrange = hazine + Du[cost[end], i]\n",
    "            if flightrange <= L\n",
    "                push!(include, i)\n",
    "            end\n",
    "        end\n",
    "        if isempty(include)\n",
    "            flag = 1\n",
    "            return Sol, flag\n",
    "        end\n",
    "\n",
    "        \n",
    "\n",
    "        distance = zeros(nNodes, nNodes)\n",
    "        for i in 1:nNodes            \n",
    "\n",
    "            if !isempty(findall(x -> x == i, TruckRoute)) # node in truck route\n",
    "                if i == 1\n",
    "                    for t in 1:tru\n",
    "                        distance[i, TruckRoute[t, 1+1]] = 1\n",
    "                    end\n",
    "                else\n",
    "                    fa_ja = findfirst(x -> x == i, TruckRoute)\n",
    "                    fa,ja=Tuple(fa_ja)\n",
    "                    if fa !== nothing && ja !== nothing\n",
    "                        distance[i, TruckRoute[fa, ja+1]] = 1\n",
    "                    end\n",
    "                end\n",
    "            end\n",
    "            \n",
    "            #added:\n",
    "            ind = findall(x -> x == i, DroneRoute)\n",
    "            if !isempty(ind) # node in drone route                \n",
    "                tuples = [Tuple(CartesianIndices(DroneRoute)[ik]) for ik in ind]\n",
    "                q1 = getindex.(tuples, 1)\n",
    "                q2 = getindex.(tuples, 2)\n",
    "                q3 = getindex.(tuples, 3)\n",
    "                for u in 1:length(q3)\n",
    "                    if q2[u] != count(!iszero, DroneRoute[q1[u], :, q3[u]])\n",
    "                        distance[i, DroneRoute[q1[u], q2[u]+1, q3[u]]] = 2\n",
    "                    end\n",
    "                end\n",
    "            end\n",
    "        end\n",
    "\n",
    "        graphic = Graphs.DiGraph(distance)\n",
    "        s = collect(all_simple_paths(graphic, 1, DroneRoute[parvaz[par], 1, uav[pahpad]]))\n",
    "        aa = []\n",
    "        for ii in 1:length(s)\n",
    "            aa = vcat(aa, s[ii])\n",
    "        end\n",
    "        for jj in 1:length(aa)\n",
    "            filter!(x -> x != aa[jj], include)\n",
    "        end\n",
    "\n",
    "        if isempty(include)\n",
    "            flag = 1\n",
    "            return Sol, flag\n",
    "        end\n",
    "    end\n",
    "\n",
    "    no = rand(1:length(include))\n",
    "    DroneRoute[parvaz[par], count(!iszero, DroneRoute[parvaz[par], :, uav[pahpad]]), uav[pahpad]] = include[no]\n",
    "\n",
    "    \n",
    "\n",
    "    Sol[\"TruckRoute\"] = TruckRoute\n",
    "    Sol[\"DroneRoute\"] = DroneRoute\n",
    "    return Sol, flag\n",
    "\n",
    "    \n",
    "end"
   ]
